    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc/core": "^1.15.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^16.4.5",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
