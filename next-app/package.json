{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "migrate:export": "ts-node --project tsconfig.scripts.json --swc supabase/scripts/export-sqlite.ts",
    "migrate:transform": "ts-node --project tsconfig.scripts.json --swc supabase/scripts/transform-data.ts",
    "migrate:import": "ts-node --project tsconfig.scripts.json --swc supabase/scripts/import-supabase.ts",
    "migrate:verify": "ts-node --project tsconfig.scripts.json --swc supabase/scripts/verify-migration.ts",
    "migrate:verify:detailed": "ts-node --project tsconfig.scripts.json --swc supabase/scripts/verify-detailed.ts",
    "migrate:reset-sequences": "ts-node --project tsconfig.scripts.json --swc supabase/scripts/reset-sequences.ts",
    "migrate:all": "npm run migrate:export && npm run migrate:transform && npm run migrate:import && npm run migrate:verify"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.80.0",
    "moment": "^2.30.1",
